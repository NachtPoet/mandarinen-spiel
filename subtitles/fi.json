{
  "subtitles": [
    {"start": 19, "end": 23, "text": "Minä piiloudun ja sinä lasket kymmeneen"},
    {"start": 23, "end": 27, "text": "Lapsuuden nauruni näkyy verhojen läpi"},
    {"start": 27, "end": 31, "text": "Avaa kaikki kaapit, vain rakkautta tässä talossa"},
    {"start": 32, "end": 34, "text": "Näytämme onnellisilta"},
    
    {"start": 34, "end": 38, "text": "Soitat kitaraa ja laulamme \"Let it be\""},
    {"start": 38, "end": 42, "text": "Rakennamme linnoja - tyynyfantasioita"},
    {"start": 42, "end": 46, "text": "Ja kun sanomme näkemiin, annat minulle mandariineja"},
    {"start": 48, "end": 50, "text": "Kunnes tapaamme jälleen"},
    
    {"start": 50, "end": 53, "text": "Mutta päivistä tuli vuosia"},
    {"start": 53, "end": 57, "text": "Välillemme rakennettiin muureja"},
    {"start": 57, "end": 61, "text": "Kaipasin sinua, niin monesti"},
    {"start": 61, "end": 64, "text": "Ja sinä minua"},
    
    {"start": 64, "end": 68, "text": "Olemme aina yhteydessä"},
    {"start": 68, "end": 72, "text": "Emme voi koskaan kadottaa toisiamme"},
    {"start": 72, "end": 76, "text": "Vaikka erossa pimeimpinä hetkinä"},
    {"start": 76, "end": 79, "text": "Se mitä meillä on ei koskaan katoa"},
    
    {"start": 80, "end": 84, "text": "Kaikkien mutkien, serpentiinien läpi"},
    {"start": 85, "end": 87, "text": "Sadepäivinä, virheet anteeksi annettuina"},
    {"start": 87, "end": 92, "text": "Olit poissa niin monta vuotta, mutta et koskaan kaukana"},
    {"start": 93, "end": 96, "text": "Minulla on aina mandariineja matkalaukussa"},
    
    {"start": 111, "end": 115, "text": "Ajatukset vapisevat, koputan ovellesi"},
    {"start": 115, "end": 119, "text": "Rennot askeleet ja siinä seisot edessäni"},
    {"start": 119, "end": 123, "text": "Sylissäsi, viimeinen palapelin palanen"},
    {"start": 124, "end": 126, "text": "Niin monet haavat paranevat"},
    
    {"start": 126, "end": 130, "text": "Hiukset harmaat mutta tuttu katse"},
    {"start": 130, "end": 134, "text": "Me molemmat nauramme, menetetty aika takaisin"},
    {"start": 134, "end": 138, "text": "Kellastuneet kuvat vanhoissa puuvitriineissä"},
    {"start": 140, "end": 142, "text": "Ja siellä kaksi mandariinia"},
    
    {"start": 142, "end": 145, "text": "Ja vuosista tuli päiviä"},
    {"start": 145, "end": 149, "text": "Muurien kivistä rakensimme tien"},
    {"start": 149, "end": 153, "text": "Kaikki menetetyt ensimmäiset kerrat"},
    {"start": 154, "end": 156, "text": "Eivät enää satu niin paljon"},
    
    {"start": 156, "end": 160, "text": "Olemme aina yhteydessä"},
    {"start": 160, "end": 164, "text": "Emme voi koskaan kadottaa toisiamme"},
    {"start": 164, "end": 168, "text": "Vaikka erossa pimeimpinä hetkinä"},
    {"start": 168, "end": 171, "text": "Se mitä meillä on ei koskaan katoa"},
    
    {"start": 173, "end": 176, "text": "Kaikkien mutkien, serpentiinien läpi"},
    {"start": 176, "end": 180, "text": "Sadepäivinä, virheet anteeksi annettuina"},
    {"start": 180, "end": 184, "text": "Olit poissa niin monta vuotta, mutta et koskaan kaukana"},
    {"start": 185, "end": 188, "text": "Minulla on aina mandariineja matkalaukussa"}
  ]
}